{
  "env": {
    "development": {
      "presets": ["env"],
      "plugins": [
        "add-module-exports"
      ]
    },
    "production": {
      "presets": ["env", ["minify", {
        "evaluate": false,
        "mangle": true
      }]],
      "plugins": [
        "add-module-exports"
      ]
    }
  }
}
